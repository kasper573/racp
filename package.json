{
  "name": "racp",
  "version": "0.0.1",
  "description": "rAthena Control Panel",
  "main": "index.js",
  "license": "UNLICENSED",
  "scripts": {
    "lint": "eslint . & prettier . --check",
    "lint:fix": "eslint . --fix & prettier . --write",
    "build": "yarn api:build && yarn app:build",
    "serve": "concurrently \"yarn api:serve\" \"yarn app:serve\"",
    "app:dev": "webpack serve",
    "app:build": "webpack",
    "app:serve:impl": "cross-var ws --spa index.html --directory dist/app --compress --log.format dev --port $appPort",
    "app:serve": "dotenv -c -- yarn app:serve:impl",
    "api:dev": "ts-node-dev --files src/api/server",
    "api:build": "yarn codegen:prisma && cd src/api && tsc --outDir ../../dist/api",
    "api:serve": "node dist/api/src/api/server.js",
    "db:deploy": "npx prisma migrate deploy",
    "codegen:prisma": "npx prisma generate",
    "codegen:rathena": "ts-node --files scripts/generateRAthenaTypes.ts",
    "test:unit": "jest src --passWithNoTests",
    "test:component": "cypress run --component",
    "test:integration": "cross-env rAthenaPath=node_modules/rathena yarn api:serve --validateResources",
    "test:e2e": "cross-var npx start-server-and-test serve $CYPRESS_BASE_URL 'cypress run'",
    "reset-data": "cd scripts && node -r @swc-node/register resetData.ts",
    "configure-rathena": "ts-node --files scripts/configureRAthena.ts",
    "analyze-bundles": "cross-env NODE_ENV=production analyzeBundles=true webpack"
  },
