  "scripts": {
    "test": "jest src --passWithNoTests",
    "lint": "eslint . & prettier . --check",
    "lint:fix": "eslint . --fix & prettier . --write",
    "app:dev": "cross-env reactRefresh=true apiBaseUrl=http://localhost:8081 appPort=8080 webpack serve",
    "app:build": "webpack",
    "app:serve": "serve dist/app -s -l tcp://0.0.0.0:$appPort",
    "app:prod": "yarn app:build && yarn app:serve",
    "api:dev": "ts-node-dev --files src/api/server --apiPort 8081",
    "api:build": "tsc -p src/api --outDir dist",
    "api:serve": "node dist/api/server.js",
    "api:prod": "yarn api:build && yarn api:serve",
    "e2e:dev": "cross-env CYPRESS_BASE_URL=http://localhost:8080 cypress open",
    "e2e:ci:dependencies": "concurrently \"yarn api:prod\" \"yarn app:prod\"",
    "e2e:ci:run": "cross-var npx start-server-and-test e2e:ci:dependencies $CYPRESS_BASE_URL 'cypress run'",
    "reset-data": "ts-node --files scripts/resetData.ts",
    "codegen": "ts-node --files scripts/generateRAthenaTypes.ts",
    "configure-rathena": "ts-node --files scripts/configureRAthena.ts",
    "analyze-bundles": "cross-env NODE_ENV=production analyzeBundles=true webpack"
  },
